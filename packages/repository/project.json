{
  "root": "packages/repository",
  "sourceRoot": "packages/repository/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/repository/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/repository"],
      "options": {
        "jestConfig": "packages/repository/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
